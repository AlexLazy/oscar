block location-map
	.location-map
		.container
			ul.tabs-menu
				li
					img(src="img/icon-car.png")
					a.btn-underline.active(href="#location-map-tab-1" data-container="location-map-tabs") На автомобиле
				li
					img(src="img/icon-subway.png")
					a.btn-underline(href="#location-map-tab-2" data-container="location-map-tabs") На метро
			#location-map-tabs
				#location-map-tab-1.tab-content(style="display:block")
