								.locations-img
									a(href="img/loc1.jpg" data-fancybox="location-map-tab-1")
										img(src="img/loc1.jpg")
									a(href="img/loc2.jpg" data-fancybox="location-map-tab-1")
										img(src="img/loc2.jpg")
